{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "pc_exam"
  },
  "apps": [{
    "root": "src",
    "outDir": "../../java/workspace/asp/web/static/examv4",
    "assets": [
      "assets"
    ],
    "index": "index.html",
    "main": "main.ts",
    "polyfills": "polyfills.ts",
    "test": "test.ts",
    "tsconfig": "tsconfig.app.json",
    "testTsconfig": "tsconfig.spec.json",
    "prefix": "app",
    "styles": [
      "styles.css",
      "assets/styles/global.css",
      "assets/styles/app.scss",
      "assets/fonts/pe/css/pe-icon-set-weather.css",
      "assets/fonts/data-table/icons.css",
      "assets/styles/cobo/cobo.common.scss",
      "assets/styles/cobo/coboSpirite/100_100/cobo.spirite.css",
      "assets/styles/cobo/coboSpirite/200_200/cobo.spirite.css",
      "assets/styles/bootstrap-pick.css"
    ],
    "scripts": [
      "../node_modules/jquery/dist/jquery.js",
      "../node_modules/popper.js/dist/umd/popper.js",
      "../node_modules/bootstrap/dist/js/bootstrap.js"
    ],
    "environmentSource": "environments/environment.ts",
    "environments": {
      "dev": "environments/environment.ts",
      "prod": "environments/environment.prod.ts"
    }
  }],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [{
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
